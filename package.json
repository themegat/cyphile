{
  "name": "cyphile",
  "displayName": "Cyphile",
  "description": "A Visual Studio Code extension to encrypt and decrypt files",
  "version": "1.0.0",
  "icon": "/assets/cyphile_icon.png",
  "author": {
    "name": "T Motsoeneng",
    "url": "https://github.com/themegat"
  },
  "publisher": "Mega-T",
  "repository": {
    "type": "git",
    "url": "https://github.com/themegat/cyphile"
  },
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Notebooks",
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cyphile.cypher",
        "title": "Cypher",
        "category": "Cyphile"
      },
      {
        "command": "cyphile.decypher",
        "title": "Decypher",
        "category": "Cyphile"
      },
      {
        "command": "cyphile.cypher-directory",
        "title": "Cypher Directory",
        "category": "Cyphile"
      },
      {
        "command": "cyphile.decypher-directory",
        "title": "Decypher Directory",
        "category": "Cyphile"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "test:coverage": "xvfb-run vscode-test --label 0 --coverage --coverage-reporter json-summary --coverage-output coverage/coverage-summary && npm run move-coverage",
    "move-coverage": "mv ./coverage/coverage-summary/coverage-summary.json ./coverage/ && rm -rf ./coverage/coverage-summary",
    "make-badges": "coverage-badges -o coverage/coverage-badge.svg"
  },
  "devDependencies": {
    "@types/cryptr": "^4.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.17.16",
    "@types/sinon": "^17.0.3",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "coverage-badges-cli": "^2.1.0",
    "eslint": "^9.16.0",
    "mocha": "^11.1.0",
    "node-coverage-badges": "^1.2.11",
    "sinon": "^19.0.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "cryptr": "^6.3.0"
  }
}
